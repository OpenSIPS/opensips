##### ----------------------------------------------- #####
### path to useful tools

sub locate_tool() {
        while ( 1 ){
               if ( -x "/usr/bin/which" ) {
                        $TOOLPATH = `which @_`;
			chomp($TOOLPATH);
                        #if ( $TOOLPATH ) {
                        #       return $TOOLPATH;
                        #}
			return;
                }

                # look in common locations
                if ( -x "/usr/bin/".@_ ){
                        $TOOLPATH = "/usr/bin/".@_;
                        return;
                }

                if ( -x "/bin/".$_[0] ) {
                        $TOOLPATH = "/bin/".$_[0];
                        return;
                }
                if ( -x "/usr/local/bin/".$_[0] ) {
                        $TOOLPATH = "/usr/local/bin/$_[0]";
                        return;
                }
	      	last;
	}
	return;
}



