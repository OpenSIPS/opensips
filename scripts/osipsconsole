	$sth = $dbh->prepare( "SELECT count(*) FROM $TABLE WHERE $COL=\'$VALUE\'" );
	
	#execute the query
	$sth->execute( );
	warn "Retrieving data from table failed", $sth->errstr( ), "\n" if $sth->err( );	
	while ( @row = $sth->fetchrow_array( ) )  {
		    $result = "@row";
 		     print "@row\n";
	}	

	$sth->finish();

        if ($result == 0) {
		$response = 0;
	} else {
		$response = 1;
	}		


}

